<ConceptMap xmlns="http://hl7.org/fhir"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <id value="FC-TelecomType"/>
  <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-wg">
    <valueCode value="cgp" />
  </extension>
  <url value="http://hl7.org/fhir/us/ccda/ConceptMap/FC-TelecomType"/>
  <version value="4.0.1"/>
  <name value="FHIRtoCCDATelecomType"/>
  <title value="FHIR to C-CDA Telecom Type"/>
  <status value="active"/>
  <experimental value="false"/>
  <publisher value="HL7 International"/>
  <description value="FHIR to C-CDA Telecom Type. In CDA, these are prefixes inside the value attribute."/>
  <contact>
    <name value="HL7 International"/>
    <telecom>
      <system value="phone"/>
      <value value="+1 734 677 7777"/>
    </telecom>
    <telecom>
      <system value="fax"/>
      <value value="+1 734 677 6622"/>
    </telecom>
    <telecom>
      <system value="email"/>
      <value value="info@hl7.org"/>
    </telecom>
  </contact>
  <sourceUri value="http://hl7.org/fhir/ValueSet/contact-point-system"/>
  <group>
    <source value="http://hl7.org/fhir/contact-point-system"/>
    <target value="http://terminology.hl7.org/CodeSystem/v3-URLScheme" />
    <element>
      <code value="email"/>
      <display value="Email"/>
      <target>
        <code value="mailto"/>
        <display value="Mailto"/>
        <equivalence value="equivalent"/>
      </target>
    </element>
    <element>
      <code value="url"/>
      <display value="URL"/>
      <target>
        <code value="http"/>
        <display value="HTTP"/>
        <equivalence value="equivalent"/>
      </target>
    </element>
    <element>
      <code value="phone"/>
      <display value="Phone"/>
      <target>
        <code value="tel"/>
        <display value="Telephone"/>
        <equivalence value="wider"/>
      </target>
    </element>
    <element>
      <code value="pager"/>
      <display value="Pager"/>
      <target>
        <code value="tel"/>
        <display value="Telephone"/>
        <equivalence value="wider"/>
        <comment value="Note that the @use should be set to PG" />
      </target>
    </element>
    <element>
      <code value="fax"/>
      <display value="Fax"/>
      <target>
        <code value="x-text-fax"/>
        <display value="Fax"/>
        <equivalence value="equivalent"/>
      </target>
    </element>
  </group>
</ConceptMap>
